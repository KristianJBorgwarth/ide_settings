{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#C591E8>\u276f</><#69FF94>\u276f</>",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#c73ecc",
          "properties": {
            "branch_icon": "",
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#5FAAE8>{{.UpstreamIcon}}\ue725(</>{{ .HEAD }}<#5FAAE8>)</>",
          "type": "git"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#69FF94>\u276f</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
